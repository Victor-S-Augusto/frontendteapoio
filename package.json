{
  "name": "react-router-template",
  "version": "1.0.0",
  "description": "Template React com React Router DOM em TypeScript",
  "main": "index.tsx",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && npm run build:copy",
    "build:copy": "if exist build (xcopy build\\* dist\\ /E /Y && rmdir /S /Q build) else echo Build folder not found",
    "build:clean": "if exist dist (rmdir /S /Q dist)",
    "build:prod": "npm run build:clean && npm run build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.15.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/node": "^20.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}